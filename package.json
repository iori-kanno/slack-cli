{
  "name": "slack-cli",
  "version": "0.1.0",
  "description": "Slack Demo",
  "license": "MIT",
  "bin": {
    "slack-cli": "dist/slack-cli.bundle.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "nodemon ./src/entry.ts",
    "build": "webpack -c webpack.js",
    "postbuild": "cp node_modules/gpt-3-encoder/vocab.bpe dist/ && cp node_modules/gpt-3-encoder/encoder.json dist/",
    "slack-cli": "node dist/slack-cli.bundle.js",
    "bot:dev": "nodemon ./src/slash-commands/index.ts",
    "bot:start": "node dist/slash-command.bundle.js"
  },
  "dependencies": {
    "dayjs": "1.11.6",
    "google-spreadsheet": "3.3.0",
    "gpt-3-encoder": "1.1.4",
    "just-order-by": "1.0.0",
    "just-shuffle": "4.2.0",
    "openai": "3.2.1"
  },
  "devDependencies": {
    "@slack/bolt": "3.13.0",
    "@slack/web-api": "6.7.2",
    "@types/configstore": "6.0.0",
    "@types/google-spreadsheet": "3.3.0",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "arg": "5.0.2",
    "colors": "1.4.0",
    "configstore": "6.0.0",
    "esbuild-loader": "2.20.0",
    "esbuild-register": "3.3.3",
    "eslint": "8.26.0",
    "eslint-config-prettier": "8.5.0",
    "just-compact": "3.1.1",
    "just-group-by": "2.2.0",
    "node-fetch": "3.2.10",
    "node-loader": "2.0.0",
    "nodemon": "2.0.20",
    "ts-loader": "9.4.1",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
